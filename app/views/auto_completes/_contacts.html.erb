<%= raw @contacts.map {|contact| {
      'id' => contact.id,
      'text' => contact.name_with_company,
      'tags' => tag_links(contact.tag_list), # PATCHED
      'name' => contact.name,
      'avatar' => avatar_to(contact, :size => 16),
      'company' => contact.is_company ? "" : contact.company.to_s,
      'email' => contact.primary_email,
      'value' => contact.id
      }
    }.to_json
%>
